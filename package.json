{
  "name": "nuxt3-app-starter",
  "version": "1.3.2",
  "description": "A Nuxt(3) app starter!",
  "author": {
    "name": "elenh",
    "email": "yisiwings@163.com"
  },
  "private": true,
  "keywords": [
    "nuxt3",
    "vue3",
    "starter",
    "ssr",
    "vite",
    "pinia"
  ],
  "bugs": {
    "url": "https://github.com/yisibell/nuxt3-app-starter/issues"
  },
  "homepage": "https://github.com/yisibell/nuxt3-app-starter",
  "scripts": {
    "pm2:docker:start": "pm2-runtime start ecosystem.config.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:docker:start-ft": "pm2-runtime start ecosystem.ft.config.js",
    "pm2:start-ft": "pm2 start ecosystem.ft.config.js",
    "dev": "node ./scripts/dev.mjs",
    "build:ft": "node ./scripts/build.mjs --env=ft",
    "build": "node ./scripts/build.mjs --env=production",
    "start:ft": "node ./scripts/app.mjs --env=ft",
    "start": "node ./scripts/app.mjs --env=production",
    "preview": "nuxt preview",
    "prepare": "nuxt prepare",
    "cleanup": "nuxt cleanup",
    "lint": "eslint .",
    "release": "changelogen --release && git push --follow-tags"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@pinia/nuxt": "^0.4.6",
    "element-plus": "^2.2.30",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.0.3",
    "@nuxt/eslint-config": "^0.1.1",
    "@pinia-plugin-persistedstate/nuxt": "^1.0.0",
    "@types/lodash": "^4.14.191",
    "changelogen": "^0.4.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^6.1.0",
    "minimist": "^1.2.7",
    "nuxt": "~3.2.0",
    "nuxt-proxy": "^0.4.1",
    "nuxt-svg-icons": "^1.9.0",
    "prettier": "^2.8.3",
    "sass": "^1.58.0"
  },
  "engines": {
    "node": ">=18.x"
  }
}
