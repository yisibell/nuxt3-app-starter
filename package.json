{
  "name": "nuxt3-app-starter",
  "version": "1.7.0",
  "description": "A Nuxt(3) app starter!",
  "author": {
    "name": "elenh",
    "email": "yisiwings@163.com"
  },
  "type": "module",
  "private": true,
  "keywords": [
    "nuxt3",
    "vue3",
    "starter",
    "ssr",
    "vite",
    "pinia"
  ],
  "bugs": {
    "url": "https://github.com/yisibell/nuxt3-app-starter/issues"
  },
  "homepage": "https://github.com/yisibell/nuxt3-app-starter",
  "scripts": {
    "pm2:docker:start": "pm2-runtime start ecosystem.config.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:docker:start-ft": "pm2-runtime start ecosystem.ft.config.js",
    "pm2:start-ft": "pm2 start ecosystem.ft.config.js",
    "dev": "node ./scripts/dev.js",
    "build:ft": "node ./scripts/build.js --env=ft",
    "build": "node ./scripts/build.js --env=production",
    "start:ft": "node ./scripts/app.js --env=ft",
    "start": "node ./scripts/app.js --env=production",
    "preview": "nuxt preview",
    "prepare": "nuxt prepare",
    "cleanup": "nuxt cleanup",
    "lint": "eslint .",
    "release": "changelogen --release && git push --follow-tags"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@pinia/nuxt": "^0.4.11",
    "element-plus": "^2.3.14",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.0.6",
    "@nuxt/eslint-config": "^0.1.1",
    "@pinia-plugin-persistedstate/nuxt": "^1.1.2",
    "@types/lodash": "^4.14.199",
    "changelogen": "^0.5.5",
    "consola": "^3.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^6.1.0",
    "minimist": "^1.2.8",
    "nuxt": "^3.7.4",
    "nuxt-proxy-request": "^1.7.0",
    "nuxt-svg-icons": "^1.21.1",
    "prettier": "^2.8.8",
    "sass": "^1.68.0"
  },
  "engines": {
    "node": ">=18.x"
  }
}